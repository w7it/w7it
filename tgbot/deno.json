    "cache": "deno cache src/main.ts"
  },
  "exports": {},
  "imports": {
    "@fluent/bundle": "npm:@fluent/bundle@^0.18.0",
    "@grammyjs/conversations": "npm:@grammyjs/conversations@^1.2.0",
    "@grammyjs/fluent": "npm:@grammyjs/fluent@^1.0.3",
    "@std/assert": "jsr:@std/assert@^1.0.2",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "grammy": "npm:grammy@^1.28.0"
  }
}
